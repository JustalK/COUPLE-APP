    "coveralls": "^3.1.0",
    "expo": "~40.0.0",
    "expo-font": "^8.4.0",
    "expo-status-bar": "~1.0.3",
    "jest-expo": "^40.0.1",
    "nyc": "^15.1.0",
    "react": "16.13.1",
    "react-dice-complete": "^1.2.1",
